{
  "name": "cybereum",
  "description": "This repository is made for Cybereum frontend",
  "private": true,
  "version": "1.0.0",
  "engines": {
    "node": "12.16.2",
    "npm": "6.14.4"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "export": "next export -o dist",
    "lint": "npm run lint:css && npm run lint:source && npm run lint:markdown",
    "lint:css": "stylelint --ignore-path .eslintignore '**/*.{js,ts,tsx}'",
    "lint:source": "eslint '**/*.{js,json,md,ts,tsx}'",
    "lint:markdown": "remark --rc-path .remarkrc.json . .github/.",
    "lint:fix": "npm run lint:fix:css && npm run lint:fix:source && npm run lint:fix:markdown",
    "lint:fix:css": "stylelint --fix --ignore-path .eslintignore '**/*.{js,ts,tsx}'",
    "lint:fix:source": "eslint --fix '**/*.{js,json,md,ts,tsx}'",
    "lint:fix:markdown": "remark --rc-path .remarkrc.json . .github/. --output",
    "prettier": "prettier  --ignore-path .gitignore --write '**/*.{js,json,ts,tsx}'",
    "prettier:check": "prettier --check  --ignore-path .gitignore '**/*.{js,json,ts,tsx}'"
  },
  "dependencies": {
    "isomorphic-unfetch": "3.0.0",
    "next": "9.3.5",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/preset-env": "7.9.5",
    "@types/node": "13.13.4",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "2.29.0",
    "@typescript-eslint/parser": "2.29.0",
    "babel-eslint": "10.1.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-plugin-flowtype": "4.7.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-json": "2.1.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-markdown": "1.0.2",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "3.0.0",
    "husky": "4.2.5",
    "prettier": "2.0.5",
    "pretty-quick": "2.0.1",
    "remark": "12.0.0",
    "remark-cli": "8.0.0",
    "remark-frontmatter": "2.0.0",
    "remark-lint": "7.0.0",
    "remark-preset-lint-markdown-style-guide": "3.0.0",
    "remark-preset-lint-recommended": "4.0.0",
    "stylelint": "13.3.3",
    "stylelint-config-standard": "20.0.0",
    "ts-loader": "7.0.1",
    "typescript": "3.8.3"
  }
}
